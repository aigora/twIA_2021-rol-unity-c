
#include <Keyboard.h>
const int Abuton=11;
const int Wbuton=10;
const int Sbuton=9;
const int Dbuton=8;
const int Fbuton=7;
const int Cbuton=6;
const int Entbuton=5;
const int FPS = 60;
void Butonscontrol(int abuton,int wbuton,int sbuton,int dbuton,int fbuton,int cbuton,int entbuton);


void setup() {
 Serial.begin(100000);
 pinMode(Abuton, INPUT_PULLUP);
 pinMode(Wbuton, INPUT_PULLUP);
 pinMode(Sbuton, INPUT_PULLUP);
 pinMode(Dbuton, INPUT_PULLUP);
 pinMode(Cbuton, INPUT_PULLUP);
 pinMode(Fbuton, INPUT_PULLUP);
 pinMode(Entbuton, INPUT_PULLUP);
 Keyboard.begin();
}

void loop() {
  // put your main code here, to run repeatedly:
  Butonscontrol(Abuton, Wbuton, Sbuton, Dbuton,Fbuton,Cbuton,Encbuton);
    delay(1000/FPS);
}
void Butonscontrol(int abuton,int wbuton,int sbuton,int dbuton,int fbuton,int cbuton,int entbuton){
  if(!digitalRead(wbuton)){
    Keyboard.press('w');
    //Keyboard.print('w');
    //Keyboard.write('w');
  } 
  
  else{
    Keyboard.release('w');
  }
  
  if(!digitalRead(abuton)){
    Keyboard.press('a');
    //Keyboard.write('a');
  }
  else{
    Keyboard.release('a');
  }
  
  if(!digitalRead(sbuton)){
    Keyboard.press('s');
    //Keyboard.write('s');
  }
  else{
    Keyboard.release('s');
  }

  if(!digitalRead(dbuton)){
    Keyboard.press('d');
    //Keyboard.write('d');
  }
  else{
    Keyboard.release('d');
  }
  if(!digitalRead(cbuton)){
    Keyboard.press('c');
    //Keyboard.write('c');
  }
  else{
    Keyboard.release('c');
  }
  if(!digitalRead(fbuton)){
    Keyboard.press('f');
    //Keyboard.write('f');
  }
  else{
    Keyboard.release('f');
  }
  if(!digitalRead(escbuton)){
    Keyboard.press('\n');
    //Keyboard.write('\n');
  }
  else{
    Keyboard.release('\n');
  }
}
